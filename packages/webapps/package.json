{
  "name": "@nielsen-media/maf-infra-environment",
  "version": "1.5.0",
  "main": "lib/index.js",
  "files": [
    "esm",
    "lib"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn build:cjs & yarn build:esm",
    "build:cjs": "BABEL_ENV=production babel --root-mode upward src --out-dir lib --extensions .ts,.tsx --delete-dir-on-start --no-comments",
    "build:esm": "BABEL_ENV=production babel --root-mode upward src --out-dir esm --extensions .ts,.tsx --delete-dir-on-start --no-comments"
  }
}
